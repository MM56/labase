<!DOCTYPE html>
<html class="{{detect.device}} {{detect.os}} {{detect.browser}} {{GLOBAL.locale}} no-js" lang="{{GLOBAL.locale}}">
	<head>
		<title>{{header.title}}</title>
		<meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<meta http-equiv="cleartype" content="on" />
		<meta http-equiv="expires" content="0" />
		<meta name="description" content="{{header.desc}}" />
		<meta name="robots" content="index, follow" />
		<meta name="revisit-after" content="3 days" />
		<meta name="audience" content="all" />
		<meta name="author" content="{{l10n.COMMON.author}}" />
		<meta name="copyright" content="{{l10n.COMMON.author}}" />
		<meta name="publisher" content="{{l10n.COMMON.author}}" />
		<!-- MOBILE -->
		<meta name='HandheldFriendly' content='true' />
		<meta name="theme-color" content="#06274f" />
		<meta name='format-detection' content='telephone=no' />
		<meta name="msapplication-tap-highlight" content="no" />
		<meta name="application-name" content="{{l10n.COMMON.webapptitle}}" />
		<meta name="apple-mobile-web-app-title" content="{{l10n.COMMON.webapptitle}}" />
		<meta name="mobile-web-app-capable" content="yes" />
		<meta name="apple-mobile-web-app-capable" content="yes" />
		<meta name="apple-mobile-web-app-status-bar-style" content="black" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no{{^GLOBAL.isDesktop}},minimal-ui{{/GLOBAL.isDesktop}}" />
		<!-- FB -->
		<meta property="og:type" content="website" />
		<meta property="og:title" content="{{l10n.COMMON.author}}" />
		<meta property="og:site_name" content="{{header.title}}" />
		<meta property="og:description" content="{{header.share}}" />
		<meta property="og:image" content="{{GLOBAL.baseURL}}{{GLOBAL.assetsPath}}{{header.img}}" />
		<meta property="og:image:width" content="1200" />
		<meta property="og:image:height" content="630" />
		<meta property="og:url" content="{{GLOBAL.baseURL}}" />
		<!-- TWITTER -->
		<meta name="twitter:card" content="summary_large_image" />
		<meta name="twitter:site" content="{{l10n.COMMON.twitter}}" />
		<meta name="twitter:title" content="{{l10n.COMMON.author}}" />
		<meta name="twitter:description" content="{{header.share}}" />
		<meta name="twitter:image" content="{{GLOBAL.baseURL}}{{GLOBAL.assetsPath}}{{header.img}}" />
		<!-- GOOGLE -->
		<meta name="google" content="notranslate" />
		<meta name="thumbnail" content="{{GLOBAL.baseURL}}{{GLOBAL.assetsPath}}img/static/share/60x60.jpg" />
		<meta itemprop="name" content="{{header.title}}" />
		<meta itemprop="description" content="{{header.share}}" />
		<!-- LINKS and ICONS -->
		<link type="text/plain" rel="author" href="/humans.txt" />
		<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
		<link rel="apple-touch-icon-precomposed" href="{{GLOBAL.assetsBaseURL}}{{GLOBAL.assetsPath}}img/static/apple-touch-icon-precomposed.png" />
		<link rel="stylesheet" type="text/css" href="{{GLOBAL.assetsBaseURL}}{{GLOBAL.assetsPath}}css/style.{{#GLOBAL.isDesktop}}desktop{{/GLOBAL.isDesktop}}{{^GLOBAL.isDesktop}}mobile{{/GLOBAL.isDesktop}}.css" media="screen, projection" />
		<script type="text/javascript">
		var appDebug = "{{debugMode}}" != "";
		var env = "{{env}}";
		var baseURL = "{{GLOBAL.baseURL}}";
		var currentURL = "{{GLOBAL.currentURL}}";
		var basePath = "{{GLOBAL.basePath}}";
		var assetsBaseURL = "{{GLOBAL.assetsBaseURL}}";
		var assetsPath = "{{GLOBAL.assetsPath}}";
		var mediasPath = "{{GLOBAL.assetsBaseURL}}{{GLOBAL.assetsPath}}medias/";
		var locale = "{{GLOBAL.locale}}";
		var defaultTemplates = {
			{{#if GLOBAL.defaultTemplates}}
			{{#GLOBAL.defaultTemplates}}
				{{{.}}}
			{{/GLOBAL.defaultTemplates}}
			{{/if}}
		};
		var isDesktop = {{#GLOBAL.isDesktop}}true{{/GLOBAL.isDesktop}}{{^GLOBAL.isDesktop}}false{{/GLOBAL.isDesktop}};
		var isTablet = {{#GLOBAL.isTablet}}true{{/GLOBAL.isTablet}}{{^GLOBAL.isTablet}}false{{/GLOBAL.isTablet}};
		var isPhone = {{#GLOBAL.isPhone}}true{{/GLOBAL.isPhone}}{{^GLOBAL.isPhone}}false{{/GLOBAL.isPhone}};
		var l10nJSON = {{{l10n_encoded}}};
		var manifestJSON = {{{manifest}}};
		var routesJSON = {{{routes}}};
		var noLocaleInRoute = {{GLOBAL.noLocaleInRoute}};
		var svgs = {{{svgs_encoded}}};
		var apiURL = "{{apiURL}}";
		</script>
	</head>
	<body>
		<div id="root" class="root-module-wrapper">
			{{> notifier/orientation }}
			{{> modules}}
			{{> loader/mainLoader }}
			{{> loader/usualLoader }}
		</div>
		<script>
			window.fbAsyncInit = function() {
				FB.init({
					appId      : '{{FBAppId}}',
					xfbml      : true,
					version    : 'v2.5',
					display	   : 'async'
				});
			};

			(function(d, s, id){
			var js, fjs = d.getElementsByTagName(s)[0];
			if (d.getElementById(id)) {return;}
			js = d.createElement(s); js.id = id;
			js.src = "//connect.facebook.net/en_US/sdk.js";
			fjs.parentNode.insertBefore(js, fjs);
			}(document, 'script', 'facebook-jssdk'));
		</script>

		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
			ga('create', '{{analytics}}', 'auto');
			ga('send', 'pageview');
		</script>
		{{>global/javascripts}}
	</body>
</html>